         <frameworkAssembly assemblyName="FSharp.Core" targetFramework="net40" />
      </frameworkAssemblies>
      <references>
         <reference file="FsRandom.dll" />
      </references>
      <tags>F# random-number computation-expression monte-carlo statistics</tags>
   </metadata>
